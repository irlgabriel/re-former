
<%= form_for @user, url: edit_user_path(@user), method: :get do |f| %>

  <%= f.label :username %>
  <%= f.text_field :username %> <br/>

  <%= f.label :email %>
  <%= f.text_field :email %> <br/>

  <%= f.label :password %>
  <%= f.password_field :password %> <br/>

  <%= f.label :username %>
  <%= f.submit :submit %> <br/>
  
<% end %>
